package day0427;

class Tv {
	private boolean power;
	private int channel;
	
	void power() {
		power = !power;
	} 
	void channelUp() {
		++channel;
	}
	void channelDown() {
		--channel;
	}
	
	class CaptionTv extends Tv {
		private boolean caption;
		void displayCaption(String text) {
			if (caption) {
				System.out.println(text);
			}
		}
	}
}

class Caption {

	public static void main(String[] args) {
		

	}

}











